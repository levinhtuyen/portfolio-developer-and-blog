{
  "name": "myblog",
  "version": "2.0.12",
  "description": "Lê Vĩnh Tuyến - Blog's của tôi - Để chút thương gửi vào trong gió, để chút buồn gửi vào trong tim...",
  "author": "TuyenLe",
  "private": false,
  "license": "MIT",
  "type": "module",
  "main": "./nuxt.config.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/levinhtuyen/myblog"
  },
  "files": [
    "assets",
    "components",
    "composables",
    "layouts",
    "modules",
    "pages",
    "public",
    "stores",
    "app.config.ts",
    "app.vue",
    "nuxt.config.ts",
    "nuxt.schema.ts",
    "tailwind.config.ts",
    "tsconfig.json"
  ],
  "scripts": {
    "build": "nuxt build ./output --preset=vercel_edge",
    "dev": "nuxt dev ./app --no-clear",
    "generate": "nuxt generate ./app",
    "preview": "nuxt preview ./app",
    "clear": "npx nuxi cleanup",
    "start": "node app/.output/server/index.mjs",
    "postinstall": "nuxt prepare ./app",
    "dev:starter": "nuxt dev ./examples/starter",
    "lint": "eslint --ext \".ts,.js,.vue\" --ignore-path .eslintignore .",
    "lint:fix": "eslint --fix --ext \".ts,.js,.vue\" --ignore-path .eslintignore .",
    "release": "release-it"
  },
  "dependencies": {
    "@nuxt/content": "^2.9.0",
    "@nuxt/devtools": "^1.0.0",
    "@nuxtjs/color-mode": "^3.3.0",
    "@nuxtjs/critters": "^0.5.1",
    "@nuxtjs/tailwindcss": "^6.8.1",
    "@pinia/nuxt": "^0.5.1",
    "@vueuse/core": "^10.5.0",
    "pdfjs-dist": "2.0.943",
    "@vueuse/nuxt": "^10.5.0",
    "nuxt-headlessui": "^1.1.4",
    "nuxt-icon": "^0.6.1",
    "nuxt-swiper": "^1.2.2",
    "pinia": "^2.1.7",
    "pm2": "^5.3.1",
    "sass": "^1.69.5",
    "stringify": "^5.2.0",
    "tailwindcss": "^3.3.5",
    "unplugin-vue-components": "^0.25.2"
  },
  "devDependencies": {
    "@iconify/json": "^2.1.143",
    "@lhci/cli": "^0.12.0",
    "@nuxt/image": "^1.8.0",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@nuxtjs/eslint-module": "^4.1.0",
    "@nuxtjs/i18n": "^8.0.0",
    "@nuxtjs/web-vitals": "^0.2.6",
    "autoprefixer": "^10.4.20",
    "consola": "^3.2.3",
    "dayjs": "^1.11.9",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "nuxt": "3.8.0",
    "nuxt-delay-hydration": "^1.2.3",
    "nuxt-gtag": "^1.1.0",
    "prettier": "^3.0.3",
    "release-it": "^16.2.1",
    "serve": "^14.2.1",
    "typescript": "^5.2.2",
    "vue-tsc": "1.8.22"
  },
  "release-it": {
    "github": {
      "release": true,
      "releaseName": "v${version}"
    },
    "git": {
      "commitMessage": "chore(release): release v${version}"
    }
  }
}
